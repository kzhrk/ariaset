{
  "name": "ariaset",
  "description": "JavaScript library for WAI-ARIA",
  "version": "0.1.2",
  "author": "kzhrk",
  "bugs": {
    "url": "https://github.com/kzhrk/ariaset/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/jest": "^23.3.12",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "coveralls": "^3.0.2",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.2",
    "regenerator-runtime": "^0.12.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "homepage": "https://github.com/kzhrk/ariaset.git",
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "keywords": [
    "WAI-ARIA",
    "accessibility",
    "aria"
  ],
  "license": "MIT",
  "main": "./dist/ariaset.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kzhrk/ariaset.git"
  },
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint --fix --ext .js ./src ./tests",
    "prepublish": "npm run build",
    "start": "NODE_ENV=development npm run webpack:dev",
    "test": "jest",
    "webpack:dev": "webpack -w",
    "webpack:pro": "webpack --display-used-exports"
  }
}
